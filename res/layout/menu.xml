<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/relativeLayout" >
    <!--  android:onClick="onRelativeLayout"-->
    
    <ToggleButton
            android:id="@+id/menu"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="15dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/button_design"
            android:onClick="onMenu"
            android:textOn=""
			android:textOff="" />

    <Button
        android:id="@+id/play"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/menu"
        android:layout_toLeftOf="@+id/menu"
        android:background="@drawable/button_play"
        android:onClick="play"/>
    
    <Button
        android:id="@+id/set"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/menu"
        android:layout_toLeftOf="@+id/play"
        android:background="@drawable/button_add"
        android:onClick="addMeasure" />

    <ToggleButton
        android:id="@+id/note"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_above="@+id/menu"
        android:layout_alignRight="@+id/menu"
        android:background="@drawable/button_note"
        android:onClick="onNote"
        android:textOn=""
		android:textOff=""/>
    <ToggleButton
        android:id="@+id/whole"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/note"
        android:layout_toLeftOf="@+id/note"
        android:background="@drawable/button_zen"
        android:onClick="onZen"
        android:textOn=""
		android:textOff=""/>
    <ToggleButton
        android:id="@+id/half"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/note"
        android:layout_toLeftOf="@+id/whole"
        android:background="@drawable/button_nibu"
        android:onClick="onNibu"
        android:textOn=""
		android:textOff="" />
    
    <ToggleButton
        android:id="@+id/quarter"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/note"
        android:layout_toLeftOf="@+id/half"
        android:background="@drawable/button_shibu"
        android:onClick="onShibu"
        android:textOn=""
		android:textOff="" />
    <ToggleButton
        android:id="@+id/eighth"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/note"
        android:layout_toLeftOf="@+id/quarter"
        android:background="@drawable/button_hatibu"
        android:onClick="onHatibu"
        android:textOn=""
		android:textOff="" />
    
    <ToggleButton
        android:id="@+id/sixteenth"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/note"
        android:layout_toLeftOf="@+id/eighth"
        android:background="@drawable/button_jurokubu"
        android:onClick="onJurokubu"
        android:textOn=""
		android:textOff="" />

    
    <ToggleButton
        android:id="@+id/rest"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_above="@+id/note"
        android:layout_alignRight="@+id/menu"
        android:background="@drawable/button_rest"
        android:onClick="onRest"
        android:textOn=""
		android:textOff=""
         />
    <ToggleButton
        android:id="@+id/whole_rest"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/rest"
        android:layout_toLeftOf="@+id/rest"
        android:background="@drawable/button_zen_rest"
        android:onClick="onZenRest"
        android:textOn=""
		android:textOff="" />
    
    
    <ToggleButton
        android:id="@+id/half_rest"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/rest"
        android:layout_toLeftOf="@+id/whole_rest"
        android:background="@drawable/button_nibu_rest"
        android:onClick="onNibuRest"
        android:textOn=""
		android:textOff=""/>
    
    <ToggleButton
        android:id="@+id/quarter_rest"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/rest"
        android:layout_toLeftOf="@+id/half_rest"
        android:background="@drawable/button_shibu_rest"
        android:onClick="onShibuRest"
        android:textOn=""
		android:textOff="" />
    <ToggleButton
        android:id="@+id/eighth_rest"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/rest"
        android:layout_toLeftOf="@+id/quarter_rest"
        android:background="@drawable/button_hatibu_rest"
        android:onClick="onHatibuRest"
        android:textOn=""
		android:textOff=""/>
    <ToggleButton
        android:id="@+id/sixteenth_rest"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignTop="@+id/rest"
        android:layout_toLeftOf="@+id/eighth_rest"
        android:background="@drawable/button_jurokubu_rest"
        android:onClick="onJurokubuRest"
        android:text="一六分休符"
        android:textOn=""
		android:textOff="" />

</RelativeLayout>
